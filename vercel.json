{
  "builds": [
    {
      "src": "src/backend/server/server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "src/backend/server/server.ts" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
